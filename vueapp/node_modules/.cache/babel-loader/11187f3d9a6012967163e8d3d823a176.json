AA,WAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAJA;AAKA,KArEA;AAsEA,IAAA,sBAtEA,oCAsEA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,sCAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAJA;AAKA;AA7EA;AAzBA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <v-sheet v-if=\"!completed\" class=\"secondary\">\n      <v-row>\n          <v-col cols=\"12\">\n            <v-text-field\n              v-model=\"company\"\n              label=\"Company Name\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"name\"\n              label=\"Name\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"role\"\n              label=\"Role\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"education\"\n              label=\"Education\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"hobbies\"\n              label=\"Hobbies\"\n              required\n            ></v-text-field>\n          </v-col>\n      </v-row>\n      <v-row>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"industry\"\n              label=\"Industry\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"elevatorpitch.mission\"\n              label=\"Mission\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"elevatorpitch.problem\"\n              label=\"Problem\"\n              required\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"6\">\n            <v-text-field\n              v-model=\"elevatorpitch.solution\"\n              label=\"Solution\"\n              required\n            ></v-text-field>\n          </v-col>\n      </v-row>\n      <v-row>\n        <v-col class=\"d-flex justify-end\">\n          <v-btn\n            @click=\"submitBiography\"\n            outlined\n            color=\"primary\"\n          >\n            Submit\n          </v-btn>\n        </v-col>\n      </v-row>\n\n      <h2>{{company}}</h2>\n      \n      <QuestionSlot \n        v-if=\"grantQuestions && grantQuestions.biography\"\n      >\n        <template v-slot:title>Biography</template> \n        <template v-slot:actions>\n          <v-btn icon color=\"primary\" @click=\"updateBiography\">\n            <v-icon large color=\"primary\">mdi-refresh</v-icon>\n          </v-btn>\n        </template> \n        <template v-slot:content>\n           <v-textarea\n            outlined\n            name=\"input-7-4\"\n            label=\"Outlined textarea\"\n            :value=\"grantQuestions.biography\"\n            v-model=\"grantQuestions.biography\"\n          ></v-textarea>\n        </template>\n      </QuestionSlot>\n      \n      <QuestionSlot \n        v-if=\"grantQuestions && grantQuestions.elevatorpitch\"\n      >\n        <template v-slot:title>Elevatorpitch</template> \n        <template v-slot:actions>\n          <v-btn icon color=\"primary\" @click=\"updateElevatorpitch\">\n            <v-icon large color=\"primary\">mdi-refresh</v-icon>\n          </v-btn>\n        </template> \n        <template v-slot:content>\n           <v-textarea\n            outlined\n            name=\"input-7-4\"\n            label=\"Outlined textarea\"\n            :value=\"grantQuestions.elevatorpitch\"\n            v-model=\"grantQuestions.elevatorpitch\"\n          ></v-textarea>\n        </template>\n      </QuestionSlot>\n  \n      <QuestionSlot \n        v-if=\"grantQuestions && grantQuestions.marketValue\" \n      >\n        <template v-slot:title>Market Value</template> \n        <template v-slot:actions>\n          <v-btn icon color=\"primary\" @click=\"updateMarketValue\">\n            <v-icon large color=\"primary\">mdi-refresh</v-icon>\n          </v-btn>\n        </template> \n        <template v-slot:content>\n           <v-textarea\n            outlined\n            name=\"input-7-4\"\n            label=\"Outlined textarea\"\n            :value=\"grantQuestions.marketValue\"\n            v-model=\"grantQuestions.marketValue\"\n          ></v-textarea>\n        </template>\n      </QuestionSlot>\n  \n      <QuestionSlot \n        v-if=\"grantQuestions && grantQuestions.marketChallenges\" \n      >\n        <template v-slot:title>Market Challenges</template> \n        <template v-slot:actions>\n          <v-btn icon color=\"primary\" @click=\"updateMarketChallenges\">\n            <v-icon large color=\"primary\">mdi-refresh</v-icon>\n          </v-btn>\n        </template> \n        <template v-slot:content>\n           <v-textarea\n            outlined\n            name=\"input-7-4\"\n            label=\"Outlined textarea\"\n            :value=\"grantQuestions.marketChallenges\"\n            v-model=\"grantQuestions.marketChallenges\"\n          ></v-textarea>\n        </template>\n      </QuestionSlot>\n      <v-row v-if=\"edit\">\n        <v-col class=\"d-flex justify-center\">\n          <v-btn\n            @click=\"complete\"\n            outlined\n            color=\"primary\"\n          >\n            Complete\n          </v-btn>\n        </v-col>\n      </v-row>\n    </v-sheet>\n\n    <v-sheet class=\"secondary\" v-if=\"completed\">\n      <h1>{{company}}</h1>\n      <h2>Biograpy</h2>\n      <v-row>\n        <v-col>\n          {{grantQuestions.biography}}\n        </v-col>\n      </v-row>\n      <h2>Elevatorpitch</h2>\n      <v-row>\n        <v-col>\n          {{grantQuestions.elevatorpitch}}\n        </v-col>\n      </v-row>\n      <h2>Market Value</h2>\n      <v-row>\n        <v-col>\n          {{grantQuestions.marketValue}}\n        </v-col>\n      </v-row>\n      <h2>Market Challenges</h2>\n      <v-row>\n        <v-col>\n          {{grantQuestions.marketChallenges}}\n        </v-col>\n      </v-row>\n    </v-sheet>\n\n    <v-overlay :value=\"overlay\">\n      <v-progress-circular\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n    </v-overlay>\n\n  </v-container>\n</template>\n\n<script>\n  import axios from 'axios';\n  import QuestionSlot from '@/components/QuestionSlot.vue';\n\n\nexport default {\n  name: 'GrantWriter',\n  components: {\n    QuestionSlot,\n  },\n  data(){\n    return{\n      company:    '',\n      name:       '',\n      education:  '',\n      role:       '',\n      hobbies:    '',\n      industry:   '',\n      elevatorpitch : {\n        mission: \"\",\n        problem: \"\",\n        solution: \"\",\n      },\n      grantQuestions: {\n      },\n      completed : false,\n      edit :false,\n      overlay: false\n    }\n  },\n  methods:{\n    complete(){\n      this.completed = true\n    },\n    submitBiography(){\n      this.overlay = true\n      const biography = {\n        company:    this.company,\n        name:       this.name,\n        role:       this.role,\n        education:  this.education,\n        hobbies:    this.hobbies.split(','),\n      }\n      //  elevatorpitch = {\"mission\":\"help musicians\",\"problem\":\"music theory is complex\",\"solution\":\"learning application with short lessons\"}\n      const elevatorpitch = {\n          mission:  this.elevatorpitch.mission,\n          problem:  this.elevatorpitch.problem,\n          solution: this.elevatorpitch.solution,\n        }\n      axios\n        .get('http://0.0.0.0:5000/',{\n            params: {\n              biography,\n              elevatorpitch,\n              industry : this.industry\n            }\n          }\n        )\n        .then(response => {\n            console.log(response)\n            this.grantQuestions = response.data\n            this.edit = true\n            this.overlay = false\n          })\n    },\n    updateBiography(){\n      this.overlay = true\n      const biography = {\n        name:       this.name,\n        role:       this.role,\n        education:  this.education,\n        hobbies:    this.hobbies.split(','),\n      }\n      axios.get('http://0.0.0.0:5000/Biography',{params: {biography : biography}})\n      .then( response => {\n        this.grantQuestions.biography = response.data \n        this.overlay = false\n      })\n    },\n    updateElevatorpitch(){\n      this.overlay = true\n      const elevatorpitch = {\n        mission:  this.elevatorpitch.mission,\n        problem:  this.elevatorpitch.problem,\n        solution: this.elevatorpitch.solution,\n      }\n      axios.get('http://0.0.0.0:5000/Elevatorpitch',{params: {elevatorpitch : elevatorpitch}})\n      .then( response => {\n        this.grantQuestions.elevatorpitch = response.data \n        this.overlay = false\n      })\n    },\n    updateMarketValue(){\n      this.overlay = true\n      axios.get('http://0.0.0.0:5000/MarketValue',{params: {industry : this.industry}})\n      .then( response => {\n        this.grantQuestions.marketValue = response.data \n        this.overlay = false\n      })\n    },\n    updateMarketChallenges(){\n      this.overlay = true\n      axios.get('http://0.0.0.0:5000/MarketChallenges',{params: {industry : this.industry}})\n      .then( response => {\n        this.grantQuestions.marketChallenges = response.data \n        this.overlay = false\n      })\n    },\n  },\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":"src/components"}]}